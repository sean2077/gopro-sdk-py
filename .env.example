# GoPro Test Configuration
# Copy this file to .env and fill in your actual values
# The .env file is gitignored and won't be committed

# Camera serial numbers (last 4 digits, comma-separated for multiple cameras)
GOPRO_TEST_CAMERAS=1234

# WiFi credentials (camera must be on the same network)
GOPRO_TEST_WIFI_SSID=your-wifi-ssid
GOPRO_TEST_WIFI_PASSWORD=your-wifi-password
